{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "React",
  "extends": "./base.json",
  "compilerOptions": {
    "target": "es6",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "module": "esnext",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "incremental": true,
    "jsx": "react-jsx",
    "typeRoots": [
      "./node_modules/@types",
      "./types"
    ],
    "types": [
      "react",
      "react-dom",
      "react-router-dom",
      "node",
      "bun-types",
    ],
  }
}