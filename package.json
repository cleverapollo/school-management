{
  "name": "tyro-web-ui",
  "version": "1.0.0",
  "description": "Monorepo for the tyro web app",
  "engines": {
    "node": ">=16.0.0 <17.0.0"
  },
  "workspaces": [
    "features/*",
    "packages/*"
  ],
  "private": true,
  "scripts": {
    "lint": "eslint --ext .ts,.tsx ./src",
    "lint:fix": "eslint --fix --ext .ts,.tsx ./src",
    "start": "set PORT=6420 && react-scripts start",
    "start:mac": "export PORT=6420 && react-scripts start",
    "build": "CI=false react-scripts build",
    "eject": "react-scripts eject",
    "clear-all": "rm -rf build node_modules",
    "re-start": "rm -rf build node_modules && yarn install && yarn start",
    "re-build": "rm -rf build node_modules && yarn install && yarn build",
    "codegen": "graphql-codegen --config codegen.ts",
    "test": "jest",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-react"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@auth0/auth0-spa-js": "^1.22.2",
    "@azure/msal-browser": "^2.28.3",
    "@azure/msal-react": "^1.4.7",
    "@emotion/cache": "^11.9.3",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@graphiql/react": "^0.13.7",
    "@graphiql/toolkit": "^0.8.0",
    "@graphql-codegen/client-preset": "1.1.4",
    "@hookform/resolvers": "^2.9.7",
    "@iconify/react": "^3.2.2",
    "@mui/icons-material": "^5.10.9",
    "@mui/lab": "^5.0.0-alpha.96",
    "@mui/material": "^5.8.6",
    "@mui/system": "^5.10.14",
    "@mui/x-date-pickers": "^5.0.4",
    "@reduxjs/toolkit": "^1.8.5",
    "@rtk-query/graphql-request-base-query": "^2.2.0",
    "@tanstack/match-sorter-utils": "^8.5.14",
    "@tanstack/react-table": "^8.5.15",
    "@types/material-ui": "^0.21.12",
    "@tyro/api": "*",
    "@tyro/configs": "*",
    "auth0-js": "^9.19.0",
    "axios": "^0.27.2",
    "change-case": "^4.1.2",
    "date-fns": "^2.28.0",
    "dayjs": "^1.11.5",
    "framer-motion": "^6.3.16",
    "graphiql": "^2.0.13",
    "graphql": "^16.6.0",
    "graphql-request": "^4.3.0",
    "history": "^5.3.0",
    "i18next": "^21.9.1",
    "i18next-browser-languagedetector": "^6.1.5",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "material-ui-popup-state": "^4.1.0",
    "mst-gql": "^0.17.1",
    "notistack": "^2.0.5",
    "nprogress": "^0.2.0",
    "numeral": "^2.0.6",
    "react": "^18.2.0",
