{
  "name": "tyro-web-ui",
  "version": "1.0.0",
  "description": "Monorepo for the tyro web app",
  "engines": {
    "node": ">=16.0.0 <17.0.0"
  },
  "workspaces": [
    "features/*",
    "packages/*"
  ],
  "private": true,
  "scripts": {
    "lint": "eslint --ext .ts,.tsx ./src",
    "lint:fix": "eslint --fix --ext .ts,.tsx ./src",
    "start": "set PORT=6420 && node scripts/start.js",
    "start:mac": "export PORT=6420 && node scripts/start.js",
    "build": "CI=false node scripts/build.js",
    "clear-all": "rm -rf build node_modules",
    "re-start": "rm -rf build node_modules && yarn install && yarn start",
    "re-build": "rm -rf build node_modules && yarn install && yarn build",
    "codegen": "graphql-codegen --config codegen.ts",
    "test": "jest",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@auth0/auth0-spa-js": "^1.22.2",
    "@azure/msal-browser": "^2.28.3",
    "@azure/msal-react": "^1.4.7",
    "@babel/core": "^7.16.0",
    "@emotion/cache": "^11.9.3",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@graphiql/react": "^0.13.7",
    "@graphiql/toolkit": "^0.8.0",
    "@graphql-codegen/client-preset": "1.1.4",
    "@hookform/resolvers": "^2.9.7",
    "@iconify/react": "^3.2.2",
    "@mui/icons-material": "^5.10.9",
    "@mui/lab": "^5.0.0-alpha.96",
    "@mui/material": "^5.8.6",
    "@mui/system": "^5.10.14",
    "@mui/x-date-pickers": "^5.0.4",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.3",
    "@reduxjs/toolkit": "^1.8.5",
    "@rtk-query/graphql-request-base-query": "^2.2.0",
    "@svgr/webpack": "^5.5.0",
    "@tanstack/match-sorter-utils": "^8.5.14",
    "@tanstack/react-query": "^4.16.1",
    "@tanstack/react-query-devtools": "^4.16.1",
    "@tanstack/react-table": "^8.5.15",
    "@types/material-ui": "^0.21.12",
    "@tyro/api": "*",
    "auth0-js": "^9.19.0",
    "axios": "^0.27.2",
    "babel-jest": "^27.4.2",
    "babel-loader": "^8.2.3",
    "babel-plugin-named-asset-import": "^0.3.8",
    "babel-preset-react-app": "^10.0.1",
    "bfj": "^7.0.2",
    "browserslist": "^4.18.1",
    "camelcase": "^6.2.1",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "change-case": "^4.1.2",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.2.0",
    "date-fns": "^2.28.0",
    "dayjs": "^1.11.5",
    "dotenv": "^10.0.0",
    "dotenv-expand": "^5.1.0",
    "eslint": "^8.3.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-webpack-plugin": "^3.1.1",
    "file-loader": "^6.2.0",
    "framer-motion": "^6.3.16",
    "fs-extra": "^10.0.0",
    "graphiql": "^2.0.13",
    "graphql": "^16.6.0",
    "graphql-request": "^4.3.0",
    "history": "^5.3.0",
